pipeline {
    agent {label "java-11"}
    stages {
        stage("url") {
            steps {
                git url: "https://github.com/yaradesikasirao/terraform2.git"
                  branch: "main"
            }
        }
        stage("init") {
            steps {
                sh "terraform init"
            }
        }
        stage("validate") {
            steps {
                sh "terraform validate"
            }
        }
        stages("autoapprove") {
            steps {
                sh "terraform apply --auto-approve"
            }
        }
    }
}